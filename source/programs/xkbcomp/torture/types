xkb_types "Common"

virtual_modifiers Alt;

type "TEST" {
    modifiers = Shift;
    map[Shift] = Level2;
    level_name[Level1]= "Any";
    level_name[Level2]= "Shift";
};

augment type "TEST" {
    modifiers = Alt;
    map[Alt] = Level2;
    level_name[level2]= "Alt";
};


